CREATE TABLE IF NOT EXISTS `dux_users` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `user_login` varchar(60) NOT NULL DEFAULT '' COMMENT '用户名',
  `user_pass` varchar(64) NOT NULL DEFAULT '' COMMENT '登录密码；sp_password加密',
  `user_nicename` varchar(50) NOT NULL DEFAULT '' COMMENT '用户美名',
  `avatar` varchar(255) NOT NULL,
  `user_email` varchar(100) NOT NULL DEFAULT '' COMMENT '登录邮箱',
  `last_login_ip` varchar(16) NOT NULL COMMENT '最后登录ip',
  `last_login_time` int(11) NOT NULL COMMENT '最后登录时间',
  `create_time` int(11) NOT NULL COMMENT '注册时间',
  `regip` varchar(22) NOT NULL,
  `user_status` int(11) NOT NULL DEFAULT '1' COMMENT '用户状态 0：禁用； 1：正常 ；2：未验证',
  `user_type` smallint(1) DEFAULT NULL COMMENT '用户类型，1:投保招标人 ;2:业务员;3：保险公司保险顾问',
  `user_code` varchar(100) NOT NULL COMMENT '身份证',
  `user_qq` int(11) NOT NULL,
  `user_tel` varchar(50) NOT NULL,
  `weixin` varchar(32) NOT NULL COMMENT '微信号',
  `unionid` varchar(32) NOT NULL,
  `user_proid` int(11) NOT NULL COMMENT '省地区id',
  `user_cityid` int(11) NOT NULL COMMENT '所在市id',
  `occupation` varchar(100) NOT NULL COMMENT '职业',
  `parent_id` int(11) NOT NULL COMMENT '父id',
  `subscribe` tinyint(4) NOT NULL COMMENT '是否已关注',
  `sex` tinyint(4) DEFAULT NULL,
  `country` varchar(22) DEFAULT NULL,
  `province` varchar(22) DEFAULT NULL,
  `city` varchar(22) DEFAULT NULL,
  `subscribe_time` int(11) DEFAULT NULL,
  `jifen` int(11) NOT NULL COMMENT '积分',
  `money` decimal(10,2) NOT NULL,
  `wbfee` float(10,2) NOT NULL COMMENT '微币',
  `historymoney` decimal(10,2) NOT NULL,
  `b_pro` varchar(20) NOT NULL,
  `b_city` varchar(20) NOT NULL,
  `b_fws` varchar(20) NOT NULL,
  `historywb` decimal(10,2) NOT NULL COMMENT '支出微币统计',
  `wsfxnum` int(11) NOT NULL COMMENT '微商文章被分享数',
  `wsrenum` int(11) NOT NULL COMMENT '微商文章被浏览数',
  `user_rank` smallint(1) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `user_login_key` (`user_login`),
  KEY `weixin` (`weixin`),
  KEY `parent_id` (`parent_id`),
  KEY `IDX_SUBSCRIBE_HISTORYMONEY` (`subscribe`,`historymoney`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 AUTO_INCREMENT=13144 ;

